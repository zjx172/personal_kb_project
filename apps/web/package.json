{
  "name": "personal-kb-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --port 5173",
    "build": "vite build",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "generate:api": "openapi --input ./openapi.json --output src/generated/api --client axios --useOptions --exportCore true --exportServices true --exportModels true --exportSchemas false",
    "api:update": "cd ../backend && python scripts/export_openapi.py -o ../openapi.json && cd ../frontend && npm run generate:api"
  },
  "dependencies": {
    "@arco-design/web-react": "^2.66.8",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sentry/react": "^10.29.0",
    "@sentry/tracing": "^7.120.4",
    "axios": "^1.6.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.556.0",
    "pdfjs-dist": "^5.4.296",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-pdf": "^10.2.0",
    "react-router-dom": "^7.10.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.16",
    "openapi-typescript-codegen": "^0.25.0",
    "postcss": "^8.4.32",
    "prettier": "^3.7.4",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.0"
  }
}
